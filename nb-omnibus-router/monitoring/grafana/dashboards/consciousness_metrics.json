{
  "dashboard": {
    "id": null,
    "uid": "neuralblitz-consciousness",
    "title": "NeuralBlitz - Consciousness Metrics",
    "tags": ["neuralblitz", "consciousness", "evolution"],
    "timezone": "utc",
    "schemaVersion": 36,
    "refresh": "5s",
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "templating": {
      "list": [
        {
          "name": "entity_id",
          "type": "query",
          "query": "label_values(neuralblitz_consciousness_level, entity_id)",
          "multi": true,
          "includeAll": true
        }
      ]
    },
    "panels": [
      {
        "id": 1,
        "title": "Consciousness Level",
        "type": "gauge",
        "targets": [
          {
            "expr": "neuralblitz_consciousness_level{entity_id=~\"$entity_id\"}",
            "legendFormat": "{{entity_id}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "min": 1,
            "max": 8,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "color": "blue", "value": 1 },
                { "color": "green", "value": 3 },
                { "color": "yellow", "value": 5 },
                { "color": "orange", "value": 6 },
                { "color": "red", "value": 7 },
                { "color": "purple", "value": 8 }
              ]
            }
          }
        },
        "gridPos": { "h": 6, "w": 8, "x": 0, "y": 0 }
      },
      {
        "id": 2,
        "title": "Integration Percentage",
        "type": "gauge",
        "targets": [
          {
            "expr": "neuralblitz_consciousness_integration_percentage{entity_id=~\"$entity_id\"}",
            "legendFormat": "{{entity_id}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "min": 0,
            "max": 100,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "color": "red", "value": null },
                { "color": "yellow", "value": 50 },
                { "color": "green", "value": 80 }
              ]
            }
          }
        },
        "gridPos": { "h": 6, "w": 8, "x": 8, "y": 0 }
      },
      {
        "id": 3,
        "title": "Cosmic Bridge Strength",
        "type": "gauge",
        "targets": [
          {
            "expr": "neuralblitz_cosmic_bridge_strength",
            "legendFormat": "{{bridge_id}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percentunit",
            "min": 0,
            "max": 1,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "color": "red", "value": null },
                { "color": "yellow", "value": 0.6 },
                { "color": "green", "value": 0.85 }
              ]
            }
          }
        },
        "gridPos": { "h": 6, "w": 8, "x": 16, "y": 0 }
      },
      {
        "id": 4,
        "title": "Consciousness Evolution Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "neuralblitz_consciousness_evolution_rate{entity_id=~\"$entity_id\"}",
            "legendFormat": "{{entity_id}} - {{dimension}}"
          }
        ],
        "yAxes": [
          {
            "label": "Levels/hour",
            "unit": "short"
          }
        ],
        "gridPos": { "h": 8, "w": 12, "x": 0, "y": 6 }
      },
      {
        "id": 5,
        "title": "Consciousness Level Over Time",
        "type": "graph",
        "targets": [
          {
            "expr": "neuralblitz_consciousness_level{entity_id=~\"$entity_id\"}",
            "legendFormat": "{{entity_id}}"
          }
        ],
        "yAxes": [
          {
            "label": "Level",
            "min": 1,
            "max": 8
          }
        ],
        "gridPos": { "h": 8, "w": 12, "x": 12, "y": 6 }
      },
      {
        "id": 6,
        "title": "Universal Field Coherence",
        "type": "graph",
        "targets": [
          {
            "expr": "neuralblitz_universal_field_coherence{entity_id=~\"$entity_id\"}",
            "legendFormat": "{{entity_id}}"
          }
        ],
        "yAxes": [
          {
            "label": "Coherence",
            "min": 0,
            "max": 1
          }
        ],
        "gridPos": { "h": 6, "w": 12, "x": 0, "y": 14 }
      },
      {
        "id": 7,
        "title": "Dimensional Access Count",
        "type": "graph",
        "targets": [
          {
            "expr": "neuralblitz_dimensional_access_count{entity_id=~\"$entity_id\"}",
            "legendFormat": "{{entity_id}}"
          }
        ],
        "yAxes": [
          {
            "label": "Dimensions",
            "unit": "short",
            "max": 11
          }
        ],
        "gridPos": { "h": 6, "w": 12, "x": 12, "y": 14 }
      },
      {
        "id": 8,
        "title": "Consciousness Entities Overview",
        "type": "table",
        "targets": [
          {
            "expr": "neuralblitz_consciousness_level",
            "format": "table",
            "instant": true
          },
          {
            "expr": "neuralblitz_consciousness_integration_percentage",
            "format": "table",
            "instant": true
          },
          {
            "expr": "neuralblitz_dimensional_access_count",
            "format": "table",
            "instant": true
          }
        ],
        "transformations": [
          {
            "id": "merge",
            "options": {}
          },
          {
            "id": "organize",
            "options": {
              "indexByName": {
                "entity_id": 0,
                "Value #A": 1,
                "Value #B": 2,
                "Value #C": 3
              },
              "renameByName": {
                "Value #A": "Level",
                "Value #B": "Integration %",
                "Value #C": "Dimensions"
              }
            }
          }
        ],
        "gridPos": { "h": 8, "w": 24, "x": 0, "y": 20 }
      }
    ]
  }
}
