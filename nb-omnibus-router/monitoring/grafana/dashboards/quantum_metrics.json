{
  "dashboard": {
    "id": null,
    "uid": "neuralblitz-quantum",
    "title": "NeuralBlitz - Quantum Metrics",
    "tags": ["neuralblitz", "quantum", "coherence"],
    "timezone": "utc",
    "schemaVersion": 36,
    "refresh": "5s",
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "templating": {
      "list": [
        {
          "name": "reality_type",
          "type": "query",
          "query": "label_values(neuralblitz_quantum_coherence_level, reality_type)",
          "multi": true,
          "includeAll": true
        },
        {
          "name": "instance_id",
          "type": "query",
          "query": "label_values(neuralblitz_quantum_coherence_level, instance_id)",
          "multi": true,
          "includeAll": true
        }
      ]
    },
    "panels": [
      {
        "id": 1,
        "title": "Quantum Coherence Level",
        "type": "gauge",
        "targets": [
          {
            "expr": "neuralblitz_quantum_coherence_level{reality_type=~\"$reality_type\", instance_id=~\"$instance_id\"}",
            "legendFormat": "{{instance_id}} - {{reality_type}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percentunit",
            "min": 0,
            "max": 1,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "color": "red", "value": null },
                { "color": "yellow", "value": 0.5 },
                { "color": "green", "value": 0.8 }
              ]
            }
          }
        },
        "gridPos": { "h": 6, "w": 8, "x": 0, "y": 0 }
      },
      {
        "id": 2,
        "title": "Entanglement Fidelity",
        "type": "gauge",
        "targets": [
          {
            "expr": "neuralblitz_quantum_entanglement_fidelity",
            "legendFormat": "{{source_reality}} â†’ {{target_reality}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percentunit",
            "min": 0,
            "max": 1,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "color": "red", "value": null },
                { "color": "yellow", "value": 0.7 },
                { "color": "green", "value": 0.9 }
              ]
            }
          }
        },
        "gridPos": { "h": 6, "w": 8, "x": 8, "y": 0 }
      },
      {
        "id": 3,
        "title": "Quantum State Evolution Rate",
        "type": "stat",
        "targets": [
          {
            "expr": "neuralblitz_quantum_state_evolution_rate",
            "legendFormat": "{{reality_layer}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "ops"
          }
        },
        "gridPos": { "h": 6, "w": 8, "x": 16, "y": 0 }
      },
      {
        "id": 4,
        "title": "Coherence Over Time",
        "type": "graph",
        "targets": [
          {
            "expr": "neuralblitz_quantum_coherence_level{reality_type=~\"$reality_type\"}",
            "legendFormat": "{{instance_id}} - {{reality_type}}"
          }
        ],
        "yAxes": [
          {
            "label": "Coherence",
            "min": 0,
            "max": 1
          }
        ],
        "gridPos": { "h": 8, "w": 12, "x": 0, "y": 6 }
      },
      {
        "id": 5,
        "title": "Quantum Simulation Duration",
        "type": "heatmap",
        "targets": [
          {
            "expr": "rate(neuralblitz_quantum_simulation_duration_seconds_bucket[5m])",
            "legendFormat": "{{le}}"
          }
        ],
        "dataFormat": "tsbuckets",
        "yBucketBound": "auto",
        "gridPos": { "h": 8, "w": 12, "x": 12, "y": 6 }
      },
      {
        "id": 6,
        "title": "Entanglement Fidelity Matrix",
        "type": "heatmap",
        "targets": [
          {
            "expr": "neuralblitz_quantum_entanglement_fidelity",
            "legendFormat": "{{source_reality}} - {{target_reality}}"
          }
        ],
        "gridPos": { "h": 8, "w": 12, "x": 0, "y": 14 }
      },
      {
        "id": 7,
        "title": "Quantum Errors",
        "type": "graph",
        "targets": [
          {
            "expr": "sum(rate(neuralblitz_quantum_errors_total[5m])) by (error_type)",
            "legendFormat": "{{error_type}}"
          }
        ],
        "yAxes": [
          {
            "label": "Errors/sec",
            "unit": "short"
          }
        ],
        "gridPos": { "h": 8, "w": 12, "x": 12, "y": 14 }
      },
      {
        "id": 8,
        "title": "Qubit Simulation Stats",
        "type": "table",
        "targets": [
          {
            "expr": "sum(rate(neuralblitz_quantum_simulation_duration_seconds_count[5m])) by (qubits, circuit_depth)",
            "format": "table",
            "instant": true
          }
        ],
        "columns": [
          { "text": "Qubits", "value": "qubits" },
          { "text": "Circuit Depth", "value": "circuit_depth" },
          { "text": "Simulations/sec", "value": "Value" }
        ],
        "gridPos": { "h": 6, "w": 24, "x": 0, "y": 22 }
      }
    ]
  }
}
